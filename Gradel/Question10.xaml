<Page x:Class="Gradel.Question10"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Gradel"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Question10" Width="724" Height="338">
    <Grid x:Name="QuestionView_Grid" Margin="3.75,-4,-3.75,-2" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition">
        <Rectangle x:Name="Question_View" RadiusY="10.5" RadiusX="10.5" StrokeThickness="3" Margin="-4,1.375,17.5,12.125">
            <Rectangle.Fill>
                <SolidColorBrush Color="{DynamicResource Question-BkgColor}"/>
            </Rectangle.Fill>
            <Rectangle.Stroke>
                <SolidColorBrush Color="{DynamicResource Color1-Blue}"/>
            </Rectangle.Stroke>
        </Rectangle>
    </Grid>
</Page>
