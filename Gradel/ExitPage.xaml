<Page x:Class="Gradel.ExitPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Gradel"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="ExitPage" Width="1292" Height="677">

    <Grid Margin="-7.5,0,0,0">
        <Rectangle Fill="#FFF4F4F5" Margin="219.5,126.5,221,83.5" RadiusY="30" RadiusX="30" StrokeThickness="2">
            <Rectangle.Stroke>
                <SolidColorBrush Color="{DynamicResource Color1-Blue}"/>
            </Rectangle.Stroke>
        </Rectangle>

    </Grid>
</Page>
